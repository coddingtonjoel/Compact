{
    "name": "compact",
    "productName": "Compact",
    "license": "MIT",
    "version": "1.0.0",
    "description": "All-in-one code minifier for desktop",
    "private": false,
    "author": {
        "name": "Joel Coddington",
        "url": "https://joelc.dev/"
    },
    "engines": {
        "node": ">=9.0.0",
        "npm": ">=5.0.0",
        "yarn": ">=1.0.0"
    },
    "main": "main.js",
    "scripts": {
        "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
        "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
        "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
        "package": "yarn run build",
        "__postpackage": "electron-packager ./ --out=./release --icon=assets/icons/win.icns --platform=win32 --arch=x64",
        "postpackage": "electron-packager ./ --out=./release --icon=assets/icons/mac.icns --overwrite"
    },
    "dependencies": {
        "@svgr/webpack": "^5.4.0",
        "bytes": "^3.1.0",
        "csso": "^4.0.3",
        "electron-log": "^4.2.1",
        "electron-react-devtools": "^0.5.3",
        "html-minifier": "^4.0.0",
        "imagemin": "^7.0.1",
        "imagemin-mozjpeg": "^9.0.0",
        "imagemin-pngquant": "^9.0.0",
        "imagemin-svgo": "^8.0.0",
        "materialize-css": "^1.0.0",
        "node-sass": "^4.14.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-toastify": "^6.0.6",
        "rimraf": "^3.0.2",
        "sass-loader": "^8.0.2",
        "terser": "^4.7.0",
        "url-loader": "^4.1.0"
    },
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@babel/preset-react": "^7.9.4",
        "babel-loader": "^8.1.0",
        "babili-webpack-plugin": "^0.1.2",
        "cross-env": "^7.0.2",
        "css-loader": "^3.5.3",
        "electron": "^15.5.5",
        "electron-devtools-installer": "^3.0.0",
        "electron-packager": "^14.2.1",
        "file-loader": "^6.0.0",
        "html-webpack-plugin": "^4.3.0",
        "mini-css-extract-plugin": "^0.9.0",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "postcss-nested": "^4.2.1",
        "postcss-preset-env": "^6.7.0",
        "postcss-pxtorem": "^5.1.1",
        "style-loader": "^1.2.0",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.11",
        "webpack-dev-server": "^3.11.0"
    }
}
